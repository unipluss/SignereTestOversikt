<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.webServer>
		<httpRedirect enabled="true" destination="https://www.signere.no" exactDestination="true" childOnly="true" httpResponseStatus="Found" />
		<httpProtocol>
			<customHeaders>			
				<remove name="X-Powered-By"/>		
				<remove name="X-AspNet-Version"/>				
				<add name="X-XSS-Protection" value="1; mode=block"/>
				<add name="X-Content-Type-Options" value="nosniff"/>
				<add name="X-Download-Options" value="noopen"/>
				<add name="X-Robots-Tag" value="noindex"/>
				<add name="X-Frame-Options" value="SAMEORIGIN" />
				<!--            <add name="Strict-Transport-Security" value="max-age=16070400; includeSubDomains; preload" />-->
				<add name="Strict-Transport-Security" value="max-age=16070400;" />
				<!--            <add name="Public-Key-Pins" value="pin-sha256=&quot;HCDB8AZxLg44vqtO7SG/R2eRlGYz3LrdnaMFnz0ie/4=&quot;;pin-sha256=&quot;/ZW/4V+a2x6mxybTe3H0UQyQrTRj6JF7cnwUq3PGH98=&quot;;pin-sha256=&quot;nqBa96p1U8PxfW/FA0/yh9ncDWbgKFKpZ+k7xJWBtzc=&quot;;pin-sha256=&quot;BGLmO9fQS3iPg86xoRPQBd+w7RxSsG2+3YIvVMaIee8=&quot;; max-age=5184000; includeSubdomains;" />-->
			</customHeaders>
		</httpProtocol>
		<staticContent>
			<clientCache cacheControlCustom="public" cacheControlMode="UseMaxAge" cacheControlMaxAge="1.00:00:00" />
		</staticContent>
		<httpCompression>
			<dynamicTypes>
				<add enabled="true" mimeType="application/json"/>
			</dynamicTypes>
			<staticTypes>
				<add enabled="true" mimeType="application/json"/>
			</staticTypes>
		</httpCompression>
		    <security>
  <!--     <ipSecurity allowUnlisted="true" denyAction="NotFound">
        <add allowed="true" ipAddress="85.200.248.222" subnetMask="255.255.0.0"/>
      </ipSecurity> -->
    </security>
		<rewrite>
			<rules>
				<rule name="Redirect to HTTPS" stopProcessing="true">
					<match url="(.*)" />
					<conditions>
						<add input="{HTTPS}" pattern="^OFF$" />
					</conditions>
					<action type="Redirect" url="https://{HTTP_HOST}/{R:1}" redirectType="Permanent" />
				</rule>
			</rules>
		</rewrite>
	</system.webServer>
</configuration>
